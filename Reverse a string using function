#include<iostream>
#include<string>
using namespace std;
string str_rev(string s){
	string reversedstr=" ";
	int n=s.length();
	for(int i=n-1;i>=0;i--)
{
	reversedstr+=s[i];
	
}
return reversedstr;

	
}
int main(){
	string s="hello";
	string stringrev=str_rev(s);
	cout<<"the reversed string is:"<<stringrev;
	return 0;
}
