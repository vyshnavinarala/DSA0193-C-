#include<iostream>
using namespace std;
int main(){
	char ch;
	cout<<"enter the character to check:\n";
	cin>>ch;
	ch=tolower(ch);
	if(ch=='a'|| ch=='e'||ch=='i'||ch=='o'||ch=='u'){
		cout<< ch<<"  is vowel";
	}
	else if(ch>='a' && ch<='z'){
		cout<<ch<<" is consonant";
	}
	else{
		cout<<"not an alphabet";
	}
	return 0;
}
